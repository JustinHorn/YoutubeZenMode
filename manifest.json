{
  "name": "Youtube Zen Mode",
  "version": "1.0",
  "manifest_version": 2,
  "matches": ["https://www.youtube.com/*"],

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "zenIcon.png"
  }
}
